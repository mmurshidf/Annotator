{
    "manifest_version": 3,
    "name": "Annotator",
    "version": "1.0.0",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "css": ["styles.css"],
        "js": ["Content.js"]
        }
    ]
}